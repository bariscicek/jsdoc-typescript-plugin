{
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "examples/misc"
        ]
    },
    "plugins": ["plugins/typescript"],
    "typescript": {
        "rootModuleName": "misc",
        "outDir": "./typings",
        "outputDocletDefs": false,
        "rewriteFunctionTypedefs": true,
        "useUnionTypeForStringEnum": true
    },
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "outputSourceReference": true
        }
    },
    "opts": {
        "recurse": true,
        "explain": false
    }
}