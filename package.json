{
  "name": "jsdoc-typescript-plugin",
  "version": "0.0.1",
  "description": "A JSDoc plugin to generate a TypeScript definition file (.d.ts) from annotated JSDoc sources",
  "scripts": {
    "tsc": "tsc",
    "example-doc": "jsdoc ./examples/jsdoc3-examples -r -c ./jsdoc.json -d ./docs/example",
    "ol-doc": "jsdoc -c ./jsdoc.ol.json -d ./docs/ol",
    "cesium-doc": "jsdoc -c ./jsdoc.cesium.json -d ./docs/cesium",
    "misc-doc": "jsdoc -c ./jsdoc.misc.json -d ./docs/misc",
    "bad-doc": "jsdoc -c ./jsdoc.bad.json -d ./docs/bad",
    "test": "npm run ol-doc && tsc ./typings/openlayers.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpinjackie/jsdoc-typescript-plugin.git"
  },
  "keywords": [
    "jsdoc",
    "typescript"
  ],
  "author": "Jackie Ng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jumpinjackie/jsdoc-typescript-plugin/issues"
  },
  "homepage": "https://github.com/jumpinjackie/jsdoc-typescript-plugin#readme",
  "devDependencies": {
    "cesium": "^1.25.0",
    "jsdoc": "^3.4.0",
    "openlayers": "^3.18.2",
    "typescript": "^2.0.3"
  }
}
