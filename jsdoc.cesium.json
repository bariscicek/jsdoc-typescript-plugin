{
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "node_modules/cesium/Source"
        ],
        "exclude": [
            "node_modules/cesium/Source/ThirdParty",
            "node_modules/cesium/Source/Workers/cesiumWorkerBootstrapper.js"
        ]
    },
    "plugins": ["plugins/cesiumTags", "plugins/typescript"],
    "typescript": {
        "rootModuleName": "cesium",
        "outDir": "./typings",
        "outputDocletDefs": false,
        "fillUndocumentedDoclets": true,
        "defaultReturnType": "void",
        "headerFile": "./content/cesium_header.ts",
        "footerFile": "./content/cesium_footer.ts",
        "doNotDeclareTopLevelElements": true,
        "initialIndentation": 1,
        "ignore": [
            "<anonymous>~ComponentDatatype",
            "<anonymous>~ComponentDatatype.BYTE",
            "<anonymous>~ComponentDatatype.UNSIGNED_BYTE",
            "<anonymous>~ComponentDatatype.SHORT",
            "<anonymous>~ComponentDatatype.UNSIGNED_SHORT",
            "<anonymous>~ComponentDatatype.FLOAT",
            "<anonymous>~ComponentDatatype.DOUBLE",
            "<anonymous>~PrimitiveType",
            "<anonymous>~PrimitiveType.POINTS",
            "<anonymous>~PrimitiveType.LINES",
            "<anonymous>~PrimitiveType.LINE_LOOP",
            "<anonymous>~PrimitiveType.LINE_STRIP",
            "<anonymous>~PrimitiveType.TRIANGLES",
            "<anonymous>~PrimitiveType.TRIANGLE_STRIP",
            "<anonymous>~PrimitiveType.TRIANGLE_FAN",
            "<anonymous>~WindingOrder",
            "<anonymous>~WindingOrder.CLOCKWISE",
            "<anonymous>~WindingOrder.COUNTER_CLOCKWISE"
        ],
        "ignoreModules": [
            "<anonymous>"
        ],
        "globalModuleAliases": [
            "<anonymous>"
        ],
        "typeReplacements": {}
    },
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "outputSourceReference": true
        }
    },
    "opts": {
        "recurse": true,
        "explain": false
    }
}