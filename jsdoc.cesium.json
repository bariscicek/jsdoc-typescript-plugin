{
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "node_modules/cesium/Source"
        ],
        "exclude": [
            "node_modules/cesium/Source/ThirdParty",
            "node_modules/cesium/Source/Workers/cesiumWorkerBootstrapper.js"
        ]
    },
    "plugins": ["plugins/cesiumTags", "plugins/typescript"],
    "typescript": {
        "rootModuleName": "cesium",
        "outDir": "./typings",
        "outputDocletDefs": false,
        "fillUndocumentedDoclets": true,
        "defaultReturnType": "void",
        "headerFile": "./content/cesium_header.ts",
        "footerFile": "./content/cesium_footer.ts",
        "doNotDeclareTopLevelElements": true,
        "initialIndentation": 1,
        "processAsEnums": {
            "classes": [
                "Color",
                "module:ClockStep",
                "module:ClockRange",
                "module:ComponentDatatype",
                "module:CornerType",
                "module:EasingFunction",
                "module:ExtrapolationType",
                "module:IndexDatatype",
                "module:Intersect",
                "module:KeyboardEventModifier",
                "module:PixelFormat",
                "module:PrimitiveType",
                "module:WindingOrder",
                "module:BingMapsStyle",
                "module:ReferenceFrame",
                "module:ScreenSpaceEventType",
                "module:TerrainQuantization",
                "module:TimeConstants",
                "module:Visibility",
                "module:BoundingSphereState",
                "module:BlendEquation",
                "module:BlendFunction",
                "module:CameraEventType",
                "module:CullFace",
                "module:DepthFunction",
                "module:HeightReference",
                "module:HorizontalOrigin",
                "module:LabelStyle",
                "module:ModelAnimationLoop",
                "module:QuadtreeTileLoadState",
                "module:SceneMode",
                "module:StencilFunction",
                "module:StencilOperation",
                "module:VerticalOrigin"
            ],
            "native": [
                
            ]
        },
        "ignore": [],
        "ignoreModules": [
            "<anonymous>"
        ],
        "globalModuleAliases": [
            "<anonymous>"
        ],
        "typeReplacements": {}
    },
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "outputSourceReference": true
        }
    },
    "opts": {
        "recurse": true,
        "explain": false
    }
}