{
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "node_modules/openlayers/externs/olx.js"
        ]
    },
    "plugins": [
        "plugins/typescript",
        "node_modules/openlayers/config/jsdoc/api/plugins/inheritdoc",
        "node_modules/openlayers/config/jsdoc/api/plugins/typedefs",
        "node_modules/openlayers/config/jsdoc/api/plugins/events",
        "node_modules/openlayers/config/jsdoc/api/plugins/observable"
    ],
    "typescript": {
        "rootModuleName": "openlayers",
        "outDir": "./typings",
        "fillUndocumentedDoclets": true,
        "ignore": [
            "ol.TileUrlFunctionType"
        ],
        "aliases": {
            "global": {
                "EsriJSONGeometry": "any",
                "GeoJSONObject": "any",
                "GeoJSONFeatureCollection": "any",
                "GeoJSONGeometry": "any",
                "GeoJSONGeometryCollection": "any"
            },
            "module": {
                "goog.events": {
                    "Event": "any",
                    "EventLike": "any",
                    "Key": "any",
                    "BrowserEvent": "any"
                },
                "goog.vec.Mat4": {
                    "Number": "any"
                },
                "ol": {
                    "TileUrlFunctionType": "(coord: ol.TileCoord, pixelRatio: number, projection: ol.proj.Projection) => string",
                    "MapEvent": "any",
                    "MapBrowserEvent": "any"
                },
                "ol.render": {
                    "Feature": "any"
                },
                "ol.source": {
                    "ImageOptions": "any",
                    "SourceOptions": "any",
                    "TileOptions": "any"
                },
                "ol.style": {
                    "ImageOptions": "any"
                }
            }
        },
        "interfaces": {
            "module": {
                "ol": {
                    "GeolocationPositionOptions": [
                        "enableHighAccuracy: boolean",
                        "maximumAge: number",
                        "timeout: number"
                    ]
                }
            }
        },
        "publicAnnotation": "api",
        "defaultReturnType": "void"
    },
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "outputSourceReference": true
        }
    },
    "opts": {
        "recurse": true,
        "explain": false
    }
}